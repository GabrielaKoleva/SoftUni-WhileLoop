import java.util.Scanner;

public class sequence2kplus1_04 {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);

        int number = Integer.parseInt(scan.nextLine());
        int n = 1;
        while (n <= number){
            System.out.println(n);
            n = n * 2 + 1;
        }
    }
}
